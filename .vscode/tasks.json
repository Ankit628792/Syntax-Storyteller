{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "npm",
            "script": "build",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$tsc",
            "presentation": {
                "reveal": "silent"
            }
        },
        {
            "label": "watch",
            "type": "npm",
            "script": "watch",
            "group": "build",
            "isBackground": true,
            "problemMatcher": "$tsc-watch",
            "presentation": {
                "reveal": "never"
            }
        },
        {
            "label": "package",
            "type": "npm",
            "script": "package",
            "group": "build",
            "presentation": {
                "reveal": "silent"
            }
        }
    ]
}